{
  "name": "@neskeep/nuxt-cms",
  "version": "1.0.0",
  "description": "A developer-friendly CMS module for Nuxt 4 with multi-database support and i18n",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "module": "./dist/module.mjs",
  "bin": {
    "nuxt-cms-init": "./scripts/init.mjs",
    "nuxt-cms-uninstall": "./scripts/uninstall.mjs"
  },
  "files": [
    "dist",
    "src",
    "scripts"
  ],
  "scripts": {
    "prepare": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "build": "nuxt-module-build build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@nuxt/kit": "^3.16.2",
    "@nuxt/ui": "^3.0.2",
    "@tiptap/extension-image": "^2.11.2",
    "@tiptap/extension-link": "^2.11.2",
    "@tiptap/extension-placeholder": "^2.11.2",
    "@tiptap/extension-text-align": "^2.11.2",
    "@tiptap/extension-underline": "^2.11.2",
    "@tiptap/pm": "^2.11.2",
    "@tiptap/starter-kit": "^2.11.2",
    "@tiptap/vue-3": "^2.11.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.7.0",
    "defu": "^6.1.4",
    "drizzle-orm": "0.38.3",
    "isomorphic-dompurify": "^2.20.0",
    "jiti": "^2.4.2",
    "jose": "^5.9.6",
    "nanoid": "^5.0.9",
    "postgres": "^3.4.5",
    "slugify": "^1.6.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.15.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.12",
    "drizzle-kit": "^0.30.1",
    "nuxt": "^3.16.2",
    "typescript": "5.7.2",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^3.1.8"
  },
  "peerDependencies": {
    "nuxt": "^3.16.0 || ^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neskeep/nuxt-cms-module"
  },
  "homepage": "https://github.com/neskeep/nuxt-cms-module#readme",
  "bugs": {
    "url": "https://github.com/neskeep/nuxt-cms-module/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Neskeep <dev@neskeep.com>",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "cms",
    "headless-cms",
    "drizzle",
    "sqlite",
    "postgresql",
    "i18n",
    "content-management"
  ]
}
